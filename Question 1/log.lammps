region cube block -${L2} ${L2} -${L2} ${L2} -${L2} ${L2}
region cube block -16.26 ${L2} -${L2} ${L2} -${L2} ${L2}
region cube block -16.26 16.26 -${L2} ${L2} -${L2} ${L2}
region cube block -16.26 16.26 -16.26 ${L2} -${L2} ${L2}
