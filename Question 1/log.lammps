variable L1 equal 4.065*8
variable L2 equal ${fccAu}*4
variable L2 equal 4.065*4
variable Ti equal 1
variable Tf equal 2500

units metal
boundary p p p
atom_style atomic

region system block -${L1} ${L1} -${L1} ${L1} -${L1} ${L1}
